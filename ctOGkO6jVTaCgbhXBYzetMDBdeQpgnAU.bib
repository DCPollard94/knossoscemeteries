@article{Bennet1990,
  title = {Knossos in {{Context}} : {{Comparative Perspectives}} on the {{Linear B Administration}} of {{LM II}}-{{III Crete}}},
  volume = {94},
  abstract = {The Linear B administration centered on Knossos in the LM III period is put into a wider context through an examination of administrative structures operating on Crete in three other periods: the Palatial, the Classical-Roman, and the Venetian periods. From this examination, two broad conclusions emerge. First, there is no natural single administrative center for the island of Crete. In periods when a single center has existed, such a system has been imposed from the outside. This observation may imply that Knossos's position as a single administrative center was inherently unstable, or was externally imposed. Second, "natural" divisions of Crete are based on topographically defined regions of varying size. When the island has been autonomous, there have been a number of such divisions forming the upper level of the administrative hierarchy. In periods when Crete has formed a part of a larger, off-island system, these same units have been incorporated at a lower level in the administrative hierarchy. It seems likely that Knossos conformed to this pattern in establishing its administrative system by incorporating preexisting units, but at a lower level. When viewed in this context, it is possible to understand features of the LM III period such as the absence of palaces and apparent disappearance of Linear A place-names.},
  number = {2},
  journal = {American Journal of Archaeology},
  author = {Bennet, John},
  year = {1990},
  keywords = {Politics,Social Structure,Texts and Inscriptions,Economy,Palaces,LMIII,Knossos},
  pages = {193-211},
  mendeley-tags = {Economy,Knossos,LMIII,Palaces,Politics,Social Structure,Texts and Inscriptions}
}

