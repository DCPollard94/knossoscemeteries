@article{coldstreamKnossosArchaicGap1999a,
  title = {Knossos: {{The Archaic Gap}}},
  volume = {94},
  abstract = {This article explores in detail the mysterious lacuna in the record of Knossos during the sixth century BC, between the abandonment of the collective tombs of the Early Iron Age and the revival of the city's fortunes in Classial times. The archaeological evidence, indicating a deep recession, is marshalled in three chronological sections: (i) the full stop in the seventh century, (ii) the Archaic gap, and (iii) recovery in the fifth century. Parallel developments at Amnisos are also discussed. In the light of any relevant clues from the written record, we evaluate the likelihood of various possible causes of the sixth-century recession at Knossos, whether natural or human. /// {$To$} {$\alpha\upsilon\tau$}\'o {$\delta\$\imath$}ota\${$\epsilon\rho\epsilon\upsilon\nu$}{\'$\alpha$} {$\lambda\epsilon\pi\tau{}o\mu\epsilon\rho\acute{\omega}\varsigma$} {$\tau{}o$} {$\mu\upsilon\sigma\tau\eta\rho\$\imath$}ota\${$\acute{\omega}\delta\epsilon\varsigma$} {$\kappa\epsilon\nu$}\'o {$\pi{}o\upsilon$} {$\delta\$\imath$}ota\${$\alpha\pi\$\imath$}ota\${$\sigma\tau\acute{\omega}\nu\epsilon\tau\alpha\$\imath$}ota\$ {$\sigma\tau\alpha$} {$\delta\epsilon\delta{}o\mu\acute{\epsilon}\nu\alpha$} {$\tau\eta\varsigma$} {$K\nu\$$}\o{}mega\${$\sigma\sigma{}o\acute{\upsilon}$} {$\kappa\alpha\tau$}{\'$\alpha$} {$\tau{}o\nu$} 60 {$\alpha\$\imath$}ota\$, {$\pi$}. {$X$}., {$\sigma\tau{}o$} {$\delta\$\imath$}ota\${\'$\alpha$}{$\sigma\tau\eta\mu\alpha$} {$\alpha\pi$}\'o {$\tau\eta\nu$} {$\epsilon\gamma\kappa\alpha\tau$}{\'$\alpha$}{$\lambda\epsilon\$\imath$}ota\${$\psi\eta$} {$\tau\$$}\o{}mega\${$\nu$} {$o\mu\alpha\delta\$\imath$}ota\${$\kappa\acute{\omega}\nu$} {$\tau$}{\'$\alpha$}{$\phi\$$}\o{}mega\${$\nu$} {$\tau\eta\varsigma$} {$\Pi\rho\acute{\omega}\$\imath$}ota\${$\mu\eta\varsigma$} {$E\pi{}o\chi\acute{\eta}\varsigma$} {$\tau{}o\upsilon$} {$\Sigma\$\imath$}ota\${$\delta\acute{\eta}\rho{}o\upsilon$} {$\mu\acute{\epsilon}\chi\rho\$\imath$}ota\$ {$\tau\eta\nu$} {$\alpha\nu\alpha\beta